VerifyHostKeyDNS ask
VisualHostKey yes
ControlMaster auto
ControlPersist 120

Host *
  ServerAliveInterval 15
  ControlPath ~/.ssh/cm.%C

Host kylo kylo.webb.haus
  HostName kylo.webb.haus
  ForwardAgent yes
  User root

Host darth darth.webb.haus
  HostName darth.webb.haus
  ForwardAgent yes
  User admin

Host 192.168.53.*
  ForwardAgent yes
  User odin

Host dweezil dweezil.webb.haus
  HostName dweezil.webb.haus
  ForwardAgent yes
  User odin

Host harvey harvey.webb.haus
  HostName harvey.webb.haus
  ForwardAgent yes
  User jordan

Host citadel citadel.shady.website shady.website
  HostName citadel.shady.website
  User jordemort

Host *.githubapp.com
  User jordemort

Host bastion.githubapp.com
  ControlPath ~/.ssh/cm.bastion

Host *.bpdev-us-east-1.github.net
  User build
  ForwardAgent yes
  RemoteForward /run/user/1001/gnupg/S.gpg-agent /Users/jordan/.gnupg/S.gpg-agent.extra

Host *.*.github.net !*.bpdev-us-east-1.github.net
  ProxyJump bastion.githubapp.com
  User jordemort

Host github.com
  User git

Host 172.19.*
  ProxyJump bastion.githubapp.com
  User build
  ForwardAgent yes
  RemoteForward /run/user/1001/gnupg/S.gpg-agent /Users/jordan/.gnupg/S.gpg-agent.extra
