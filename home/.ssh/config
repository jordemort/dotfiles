VerifyHostKeyDNS ask
VisualHostKey yes
ForwardAgent no
ControlMaster auto
ControlPath ~/.ssh/cm.%C
ControlPersist 120

Host *
  ServerAliveInterval 15

Host kylo kylo.webb.haus
  HostName kylo.webb.haus
  ForwardAgent yes
  User root

Host darth darth.webb.haus
  HostName darth.webb.haus
  ForwardAgent yes
  User admin

Host ciatdel citadel.shady.website shady.website
  HostName citadel.shady.website
  User jordemort

Host *.githubapp.com
  User jordemort

Host *.github.net
  ProxyJump bastion.githubapp.com
  User jordemort

Host github.com
  User git
