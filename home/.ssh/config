VerifyHostKeyDNS ask
VisualHostKey yes
ControlMaster auto
ControlPersist 120

Host *
  ServerAliveInterval 15
  ControlPath ~/.ssh/cm.%C

Host kylo kylo.webb.haus
  HostName kylo.webb.haus
  ForwardAgent yes
  User root

Host darth darth.webb.haus
  HostName darth.webb.haus
  ForwardAgent yes
  User admin

Host citadel citadel.shady.website shady.website
  HostName citadel.shady.website
  User jordemort

Host *.githubapp.com
  User jordemort

Host bastion.githubapp.com
  ControlPath ~/.ssh/cm.bastion

Host *.github.net
  ProxyJump bastion.githubapp.com
  User jordemort

Host github.com
  User git

Host 172.19.*
  ProxyJump bastion.githubapp.com
  User admin
  ForwardAgent yes
  RemoteForward /run/user/1000/gnupg/S.gpg-agent /Users/jordan/.gnupg/S.gpg-agent.extra
